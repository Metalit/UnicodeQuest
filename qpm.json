{
  "version": "0.1.0",
  "sharedDir": "shared",
  "dependenciesDir": "extern",
  "info": {
    "name": "Unicode",
    "id": "unicode",
    "version": "0.1.4",
    "url": null,
    "additionalData": {
      "overrideSoName": "libunicode.so"
    }
  },
  "workspace": {
    "scripts": {
      "build": [
        "pwsh ./scripts/build.ps1 $0?"
      ],
      "clean": [
        "pwsh ./scripts/build.ps1 -clean"
      ],
      "copy": [
        "pwsh ./scripts/copy.ps1 $0:?",
        "pwsh ./scripts/restart-game.ps1"
      ],
      "log": [
        "pwsh ./scripts/start-logging.ps1 $0:"
      ],
      "qmod": [
        "pwsh ./scripts/build.ps1 $0?",
        "pwsh ./scripts/createqmod.ps1"
      ],
      "restart": [
        "pwsh ./scripts/restart-game.ps1"
      ],
      "stack": [
        "pwsh ./scripts/ndk-stack.ps1 $0?"
      ],
      "tomb": [
        "pwsh ./scripts/pull-tombstone.ps1 -analyze"
      ]
    },
    "qmodIncludeDirs": [
      "./build",
      "./extern/libs"
    ],
    "qmodIncludeFiles": [],
    "qmodOutput": "./Unicode.qmod"
  },
  "dependencies": [
    {
      "id": "beatsaber-hook",
      "versionRange": "^6.4.0",
      "additionalData": {}
    },
    {
      "id": "scotland2",
      "versionRange": "^0.1.6",
      "additionalData": {
        "includeQmod": false
      }
    },
    {
      "id": "bs-cordl",
      "versionRange": "^4004.0.0",
      "additionalData": {}
    },
    {
      "id": "paper2_scotland2",
      "versionRange": "^4.5.0",
      "additionalData": {}
    },
    {
      "id": "metacore",
      "versionRange": "^1.0.0",
      "additionalData": {}
    }
  ]
}
